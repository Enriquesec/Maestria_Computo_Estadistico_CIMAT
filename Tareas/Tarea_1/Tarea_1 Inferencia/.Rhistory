data_uniforme <- data_frame(x=seq(0:n)) %>%
mutate("f_x"=1/n, "F_x"= cumsum(f_x))
cdf <- ggplot(data=data_uniforme, aes(x))+stat_ecdf()+labs(y="F(x)",title="CDF Unifrome Discreta" )
pdf <- ggplot(data=data_uniforme)+geom_line(aes(x, f_x))+labs(x="x", y="f(x)",title = "PDF Unifrome Discreta")
grid.arrange(pdf, cdf, ncol=2)
}
